   -
     name : ArretServeur
     internal_properties : CAAAAAgAAADgE0Snkg0jubsewPaICIva5yDGwGJqoDboeUb7jJEB5HdrAImuWXy0LvWVXVM5mT/UGjGyzjg7uXxk2ShdyJutd9dRRCGWkmAWwbOccKZU0gBGuEnDAw8VAMLRBujWLbZgqKVwlqGwg25krlnf6880FbqrCIw3NAz+gMfMuS1dC2CjZiVc1DOtP/FHu/Hmojp3Nk9ZDlv29F6idKttdKmlVA==
     procedure_id : 4362986513037144733
     type_code : 14
     code : |1-
      Procédure ArretServeur()
      
      // Arrêt du timer de rafraichissement
      FinAutomatismeProcédure(RaffraichissementTable)
      
      // Arrêt du thread d'écoute
      FinAutomatismeProcédure(EcouteConnexions)
      
      UTILISER sectiongestiontable dans 
      	
      	// On arrête tous les threads en cours 
      	sNomSocket est une chaîne
      	
      	SignalModifie("TERM",signalOuvert)
      	
      	// Arrêt des différent threads
      	pour toute ligne de Table1
      		// On récupère les paramètres de cette connexion
      		sNomSocket = Socket
      		
      		// Fermeture de la socket
      		Si SocketExiste(sNomSocket) = vrai alors
      			SocketFerme(sNomSocket)
      		FIN
      	FIN
      	
      	ThreadPause(2)
      fin
      
      // Fermeture de la socket d'écoute
      quand exception dans
      	SocketFerme(csNomSocketEcoute)
      faire
      	FinProgramme(vrai)
      fin
