     name : EcouteConnexions
     internal_properties : CAAAAAgAAACs/f6EHjW8W4k4xu08e9/VLcQW04MzAVEXzjJVhfWvhEVKVX6J855f9aFipXflon2Z19mRTnWAGdKiL7hQUceDpKOtxOjfnvyIzGdiy2diQjuM1D5t9dHdfU6a9p50DuRahaioPl4FX2X8dG2ZkDPTSsSSM348Cm6JKoGZ26G/33yYkjJGE020T3Fp4yLZJ9P0R+Jbgyl1lA==
     procedure_id : 4360394324521576228
     type_code : 14
     code : |1-
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle s'exécutera dans un thread sécurisé (sans avoir besoin d'appeler la fonction ThreadExecute), sans utilisation de HFSQL
      // 
      Procédure EcouteConnexions()
      
      sNomCanal est une chaine
      sNomThread est une chaine
      
      // On attend les connexion en permanence
      Boucle	
      	threadpause(2)
      	
      	Si SocketAttendConnexion(csNomSocketEcoute) alors
      		
      		// Nouvelle demande de connexion
      		// Dans cet exemple, on accepte les connexions
      		sNomCanal = SocketAccepte(csNomSocketEcoute)
      		
      		// Vérification de l'opération d'acceptation
      		Si sNomCanal~="" alors
      			ErreurAsynchrone(<§$0001§>,ErreurInfo())
      			SORTIR
      		SINON
      			// On lance un thread pour gérer cette connexion
      			sNomThread = sNomCanal
      			GestionConnexion(sNomCanal)
      						
      		FiN
      	FIN	
      FIN
     type : 458752
   -
     name : GestionConnexion
     internal_properties : CAAAAAgAAADG0S4vfne17JjSuPqsBxsGIbBUlblts2BSPD042K3tqPQUY47NSMm69IFU2kbymTTuWtqvkb8oFNkt+6qLauwghy8fDM0ifNCCqg+Ncf5SZHcrqZXkH0WH40Gqh/bO7Ys4mPG3imUQI7jtygILQHuzDbDN/nHPgdqSE4MkGoUazDFklV3Z
     procedure_id : 4360399393603839879
     type_code : 14
     code : |1-
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle s'exécutera dans un thread sécurisé (sans avoir besoin d'appeler la fonction ThreadExecute), sans utilisation de HFSQL
      // 
      Procédure GestionConnexion(sNomConnexion)
      
      sMessage est une chaine
      sIp est une chaine = SocketClientInfo(sNomConnexion,SocketAdresse)
      
      // Lecture des messages
      Boucle
      	
      	si SignalAttend("TERM",5) alors sortir
      	
      	MettreAJourTable(sNomConnexion,sIp)
      	sMessage = SocketLit(sNomConnexion,Faux,10)
      	
      	SELON sMessage
      		CAS "BYE" :
      			// Sortie de l'utilisateur
      			SORTIR
      			
      		CAS "" :
      			// Ce n'est pas une lecture bloquante, on retourne dans la boucle
      			threadpause(1)
      						
      		AUTRES CAS :
      			// Message à poster			
      			GestionMessage(sMessage)			
      	FIN
      FIN
      
      // Fermeture de la socket
      SocketFerme(sNomConnexion)
     type : 458752
   -
     name : MettreAJourTable
     procedure_id : 4360400643574880517
     type_code : 14
     code : |1-
      Procédure MettreAJourTable(sNomSocket,sIP) <thread principal>
      
      nIndice est un entier
      
      
      UTILISER SectionGestionTable DANS
      
      	// On recherche si cette connexion est déjà référencé dans la table
      	nIndice = TableCherche(Table1.Socket,sNomSocket)
      	
      	// Nouvelle connexion ?
      	Si nIndice = -1 alors
      		// Nouvelle connexion
      		Table1.AjouteLigne(sIP,sNomSocket,Maintenant())
      	SINON
      		// Mise à jour de connexion
      		Table1.ModifieLigne(nIndice,sIP,sNomSocket,Maintenant())
      	FIN
      fin
     type : 458752
   -
     name : GestionMessage
     procedure_id : 4360413338370011633
     type_code : 14
     code : |1+
      Procédure GestionMessage(sMessage) <thread principal>
      
      UTILISER SectionGestionTable DANS
      	
      	sNomSocket est une chaine
      	
      	// On renvoie le message à tous les connectés
      	pour toute ligne de Table1
      		
      		// On récupère le nom de la socket
      		sNomSocket = Socket
      		
      		// On vérifie l'existence de cette socket 
      		Si SocketExiste(sNomSocket) alors		
      			// On envoie le message
      			SocketEcrit(sNomSocket,sMessage)	
      		FIN
      	FIN
      FIN
     type : 458752
